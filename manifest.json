{
  "name": "Simple Search Results",
  "description": "Disable AI summaries on all Google search results.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "icon.png"
  },
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules_1.json"
    }]
  },
 "permissions": ["declarativeNetRequest"],
 "host_permissions": [
    "https://*.google.com/search*"
  ]
}